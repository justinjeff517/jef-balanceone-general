{
  "schema": [
    {
      "data": {
        "id": {
          "type": "string",
          "format": "uuid"
        },
        "name": {
          "type": "string",
          "minLength": 1
        },
        "slug": {
          "type": "string",
          "pattern": "^[a-z0-9_-]+$"
        },
        "tin": {
          "type": "string",
          "minLength": 1
        },
        "address": {
            "type":"string"
        }
      }
    }
  ]
}
